{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_source",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_destination",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          DateRptd as string,",
				"          DRNO as string,",
				"          DATEOCC as string,",
				"          TIMEOCC as string,",
				"          AREA as string,",
				"          AREANAME as string,",
				"          RD as string,",
				"          CrmCd as string,",
				"          CrmCdDesc as string,",
				"          Status as string,",
				"          StatusDesc as string,",
				"          LOCATION as string,",
				"          CrossStreet as string,",
				"          Location1 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}